{
  "family": "mlopsRL",
  "containerDefinitions": [
    {
      "name": "projetmlopsConteneur",
      "image": "939471053513.dkr.ecr.eu-north-1.amazonaws.com/wahibma/projet_mlops:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "bridge",
  "executionRoleArn": "arn:aws:iam::939471053513:role/ecsTaskExecutionRole"
}
